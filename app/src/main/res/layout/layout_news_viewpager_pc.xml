<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout
    android:id="@+id/news_viewpager_pc_swiperefreshlayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".activity.ActivityMain">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="5dp"
            android:gravity="center">
            <com.fly.tkuilife.view.TextRoundProgress
                android:id="@+id/news_viewpager_pc_pc_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>
            <com.fly.tkuilife.view.TextRoundProgress
                android:id="@+id/news_viewpager_pc_pc_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>
            <com.fly.tkuilife.view.TextRoundProgress
                android:id="@+id/news_viewpager_pc_pc_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="5dp"
            android:gravity="center">
            <com.fly.tkuilife.view.TextRoundProgress
                android:id="@+id/news_viewpager_pc_pc_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>
            <com.fly.tkuilife.view.TextRoundProgress
                android:id="@+id/news_viewpager_pc_pc_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>
            <com.fly.tkuilife.view.TextRoundProgress
                android:id="@+id/news_viewpager_pc_pc_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="5dp"
            android:gravity="center">
            <com.fly.tkuilife.view.TextRoundProgress
                android:id="@+id/news_viewpager_pc_pc_7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>
            <com.fly.tkuilife.view.TextRoundProgress
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:visibility="invisible"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>
            <com.fly.tkuilife.view.TextRoundProgress
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:visibility="invisible"
                app:max="0"
                app:progress="0"
                app:numSize="15dp"
                app:progressColor="#C5832C"
                app:progressWidth="20dp"
                app:roundColor="#707070"
                app:roundWidth="20dp"
                app:startAngle="0"
                app:text="-"
                app:textSize="12dp"
                app:textShow="true"
                app:textColor="@color/colorTextPrimary">
            </com.fly.tkuilife.view.TextRoundProgress>

        </LinearLayout>
    </LinearLayout>


</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>